#---install packages---#
install.packages(googledrive)
install.packages(readr)

# --- Load Libraries --- #
library(googledrive)
library(readr)

# --- Download data --- #
data_id <- "https://drive.google.com/file/d/1XLykKUpB-szp7dtdZVyEiCg7o4yw16lz/view?usp=sharing" 
out_file <- "Amsterdamlistings.csv" 

drive_download(
    as_id(data_id), 
    path = out_file,
    overwrite = TRUE)
